<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mintytech.microlite20charactergeneratorii.app" >

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="3" />

        <activity
            android:name="com.mintytech.microlite20charactergeneratorii.app.SheetListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mintytech.microlite20charactergeneratorii.app.SheetDetailActivity"
            android:label="@string/title_sheet_detail"
            android:parentActivityName="com.mintytech.microlite20charactergeneratorii.app.SheetListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mintytech.microlite20charactergeneratorii.app.SheetListActivity" />
        </activity>
        <activity
            android:name="com.mintytech.microlite20charactergeneratorii.app.ChooseRollActivity"
            android:label="@string/title_activity_choose_roll" >
        </activity>
        <activity
            android:name="com.mintytech.microlite20charactergeneratorii.app.RollActivity"
            android:label="@string/title_activity_roll" >
        </activity>
    </application>

</manifest>
